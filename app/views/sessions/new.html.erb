<div class="card cardsn" style="width:400px">
<%= form_with(scope: :session, url: sessions_path, local: true) do |f| %>
    <div class="card-body ">
<%= f.label :email %>
<%= f.email_field :Email, class:"form-control" %><br>
<%= f.label :password %>
<%= f.password_field :password,class:"form-control" %><br>
<%= f.submit "Log in", class: "btn btn-primary form-control" %>
</div>
</div>
<% end %>


